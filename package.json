{
  "name": "vidnova",
  "private": true,
  "license": "CC-BY-NC-4.0",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "docker": "docker compose up",
    "db": "docker compose up -d ecorally-db && docker compose up -d ecorally-redis && docker compose up -d vidnova-geo-db",
    "prepare": "husky"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "turbo": "^2.5.3",
    "typescript": "5.8.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@1.22.22",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@types/jest": "^29.5.14",
    "eslint": "^9.27.0",
    "jest": "^29.7.0",
    "typescript-eslint": "^8.33.0"
  }
}
