{
  "extends": "../tsconfig/base.json",
  "include": [
    "."
  ],
  "compilerOptions": {
    "strictNullChecks": true,
    "types": [
      "node"
    ],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "outDir": "dist",
    "strictPropertyInitialization": false
  },
  "exclude": [
    "dist",
    "build",
    "node_modules"
  ]
}